<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.tms.dz32.Horse" name="horse1"></bean>
    <bean class="org.tms.dz32.Horse" name="horse2"></bean>
    <bean class="org.tms.dz32.Horse" name="horse3"></bean>

    <bean class="org.tms.dz32.Rider" name="rider1"></bean>
    <bean class="org.tms.dz32.Rider" name="rider2"></bean>
    <bean class="org.tms.dz32.Rider" name="rider3"></bean>

    <bean class="org.tms.dz32.Pair" name="pair1" autowire="byType" init-method="createPairs"></bean>


</beans>